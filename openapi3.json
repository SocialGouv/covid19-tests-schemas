{
  "openapi": "3.0.0",
  "info": {
    "title": "Schema de définition des tests au COVID-19",
    "description": "Detail du schéma de données test COVID-19",
    "version": "1.0.0"
  },
  "paths": {
    "/test": {
      "post": {
        "description": "Création d'un nouveau test au COVID-19",
        "responses": {
          "201": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PostTestResponse"
                }
              }
            },
            "description": "Orientation"
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PostTestRequest"
              }
            }
          },
          "required": true
        }
      }
    }
  },
  "components": {
    "schemas": {
      "type": "object",
      "PostTestRequest": {
        "description": "Corps de la requête POST pour enregistrer le test",
        "required": [
          "orientation"
        ],
        "properties": {
          "test": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/Test"
              }
            ]
          }
        }
      },
      "PostTestResponse": {
        "type": "object",
        "description": "Réponse de l'envoi du test",
        "example": {
          "data": {
            "success": true
          }
        }
      },
      "Test": {
        "type": "object",
        "required": [
          "nom",
          "statut",
          "technologie"
        ],
        "properties": {
          "nom": {
            "title": "Nom du test",
            "type": "string"
          },
          "producteur": {
            "title": "Producteur",
            "type": "string",
            "example": [
              "PHILIPS",
              "THOMSON",
              "AKAI"
            ]
          },
          "paysProduction": {
            "title": "Pays de production",
            "$ref": "#/components/schemas/Pays"
          },
          "statut": {
            "title": "Statut du test",
            "$ref": "#/components/schemas/Statut"
          },
          "distributeurFrance": {
            "$ref": "#/components/schemas/Distributeur"
          },
          "normes": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Norme"
            }
          },
          "natureProduit": {
            "$ref": "#/components/schemas/NatureProduit"
          },
          "technologie": {
            "$ref": "#/components/schemas/TechnologieTest"
          },
          "typeReactif": {
            "$ref": "#/components/schemas/TypeReactif"
          },
          "typeCible": {
            "$ref": "#/components/schemas/TypeCible"
          },
          "nombresCibles": {
            "$ref": "#/components/schemas/NombreCibles"
          },
          "machinesNecessaires": {
            "title": "Machines nécessaires",
            "type": "string"
          },
          "ouvertureTest": {
            "$ref": "#/components/schemas/OuvertureTest"
          },
          "courbeCinetiquePositive": {
            "title": "Courbe type de cinétique de positivité du test",
            "type": "string"
          },
          "typePrelevement": {
            "$ref": "#/components/schemas/TypePrelevement"
          },
          "modalitesAcheminementPrelevements": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ModaliteAcheminementPrelevement"
            }
          },
          "typeStructureCapable": {
            "title": "Type de structures en mesure de faire le test",
            "type": "string",
            "minimum": 0,
            "maximum": 1000000
          },
          "nombreStructuresCapablesFrance": {
            "title": "Nombre de structures en mesure de faire le test sur le territoire nationnal",
            "type": "integer",
            "minimum": 0,
            "maximum": 1000000
          },
          "nombreAnalysesPossiblesJour": {
            "title": "Nombre d'analyses possibles au niveau nationnal par jour (si le consommable n'est pas un facteur limitant). (Au jour de l'analyse)",
            "type": "integer",
            "minimum": 0,
            "maximum": 100000
          },
          "nombreAnalysesPossiblesAvenirProche": {
            "title": "Nombre d'analyses possibles au niveau national par jour (si le consommable n'est pas un facteur limitant). (dans un avenir proche (moins d'un mois)",
            "type": "integer",
            "minimum": 0,
            "maximum": 100000
          },
          "vitesseAnalyse": {
            "title": "Vitesse d'analyse en minutes",
            "type": "integer"
          },
          "valeurPredictivePositive": {
            "title": "Valeur Prédictive positive",
            "type": "integer"
          },
          "valeurPredictiveNegative": {
            "title": "Valeur Prédictive négative",
            "type": "integer"
          },
          "sensibilite": {
            "title": "Sensibilité",
            "type": "integer"
          },
          "specificite": {
            "title": "Spécificité",
            "type": "integer"
          },
          "facteursDeVariation": {
            "title": "Facteurs faisant varier VPP/VPN/Se/Sp",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/FacteurDeVariation"
            }
          },
          "fourchettePrixBasse": {
            "title": "Fourchette de prix basse du test (par test)",
            "type": "integer"
          },
          "fourchettePrixHaute": {
            "title": "Fourchette de prix basse du test (par test)",
            "type": "integer"
          },
          "disponibiliteNationale": {
            "$ref": "#/components/schemas/DisponibiliteNationale"
          },
          "disponibiliteInternationale": {
            "$ref": "#/components/schemas/DisponibiliteInternationale"
          },
          "usagesTest": {
            "title": "Usages du test",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/UsageTest"
            }
          },
          "strategiesCanauxDistribution": {
            "title": "Stratégies de canaux de distribution des tests",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/StrategieCanauxDistribution"
            }
          },
          "statutRemboursement": {
            "title": "Statut du remboursement",
            "$ref": "#/components/schemas/StatutRemboursement"
          },
          "signalementsReactovigilance": {
            "title": "Signalements de réactovigilance",
            "type": "string"
          },
          "pointsContact": {
            "title": "Points de contact",
            "type": "string"
          },
          "commentaires": {
            "title": "Commentaires",
            "type": "string"
          },
          "documents": {
            "title": "Fiche produits et documents",
            "items": {
              "$ref": "#/components/schemas/File"
            }
          }
        }
      },
      "Pays": {
        "description": "Pays à la norme ISO3361-alpha2",
        "example": "FR",
        "title": "Pays",
        "type": "string"
      },
      "Distributeur": {
        "title": "Distributeur en France",
        "type": "object",
        "properties": {
          "nom": {
            "title": "Nom du distributeur",
            "type": "string"
          },
          "nationalite": {
            "title": "Nationalité du distributeur",
            "$ref": "#/components/schemas/Pays"
          }
        }
      },
      "Norme": {
        "enum": [
          "CE",
          "International",
          "Validation CNR",
          "Validation ANSM",
          "Inconnue"
        ],
        "title": "Norme / Marquage",
        "type": "string"
      },
      "NatureProduit": {
        "enum": [
          "Réactif",
          "Autre dispositif"
        ],
        "title": "Nature du produit",
        "type": "string"
      },
      "TechnologieTest": {
        "enum": [
          "RT-PCR",
          "Test rapide",
          "Elisa",
          "Autotest"
        ],
        "title": "Technologie de test",
        "type": "string"
      },
      "TypeReactif": {
        "enum": [
          "Amplification",
          "Extraction"
        ],
        "title": "Type de réactif",
        "type": "string"
      },
      "TypeCible": {
        "enum": [
          "Anticorps",
          "Antigènes"
        ],
        "title": "Type de cible",
        "type": "string"
      },
      "NombreCibles": {
        "enum": [
          1,
          2
        ],
        "title": "Nombre de cibles",
        "type": "integer"
      },
      "OuvertureTest": {
        "title": "Test ouvert / fermé",
        "enum": [
          "Ouvert",
          "Fermé",
          "NC"
        ],
        "type": "string"
      },
      "TypePrelevement": {
        "title": "Type de prélèvement nécessaire au test",
        "enum": [
          "Sérum",
          "Plasma",
          "Sang",
          "Micro-gouttes capillaires",
          "Naso-pharyngé",
          "Salive",
          "Selles",
          "Larmes",
          "Autres"
        ],
        "type": "string"
      },
      "ModaliteAcheminementPrelevement": {
        "title": "Modalité d'acheminement des prélèvements",
        "enum": [
          "Emballage simple",
          "Triple emballage",
          "Chaîne du froid",
          "Autres"
        ],
        "type": "string"
      },
      "FacteurDeVariation": {
        "title": "Facteur faisant varier VPP/VPN/Se/Sp",
        "type": "string",
        "enum": [
          "Immunodépression",
          "Grossesse",
          "Âge",
          "Immunité croisée",
          "Autre"
        ]
      },
      "DisponibiliteNationale": {
        "title": "Nombre de tests disponibles au niveau national par mois",
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/DisponibiliteNationaleDate"
        }
      },
      "DisponibiliteNationaleDate": {
        "type": "object",
        "properties": {
          "count": {
            "title": "Nombre de tests disponibles au niveau national",
            "type": "integer"
          },
          "date": {
            "example": "2019-12-25",
            "title": "Date de la donnée",
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "DisponibiliteInternationale": {
        "title": "Capacité de production par semaine",
        "type": "object",
        "properties": {
          "count": {
            "title": "Nombre de tests disponibles par semaine",
            "type": "integer"
          },
          "modalites": {
            "title": "Modalités d'acheminement",
            "type": "string"
          },
          "delais": {
            "title": "Delais disponibles pour la livraison",
            "type": "string"
          },
          "pays": {
            "$ref": "#/components/schemas/Pays"
          },
          "date": {
            "example": "2019-12-25",
            "title": "Date de la donnée",
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "UsageTest": {
        "title": "Usage du test",
        "type": "string",
        "enum": [
          "Diagnostic pour prise en charge individuelle",
          "Diagnostic pour cohorting",
          "Diagnostic pour levée d'isolement personnel",
          "Etude populationnelle pour guider les stratégies",
          "Autre"
        ]
      },
      "StrategieCanauxDistribution": {
        "title": "Stratégie de canaux de distribution des tests",
        "type": "string",
        "enum": [
          "Hôpital",
          "Médecine de ville",
          "Laboratoires de ville",
          "Pharmacies",
          "Autre"
        ]
      },
      "Statut": {
        "type": "string",
        "enum": [
          "Non validé",
          "En cours d’évaluation",
          "Validé",
          "Inconnu"
        ]
      },
      "StatutRemboursement": {
        "type": "string",
        "enum": [
          "Remboursé",
          "Non remboursé"
        ]
      },
      "File": {
        "type": "string",
        "format": "binary"
      }
    }
  }
}
